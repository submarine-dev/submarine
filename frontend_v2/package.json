{
  "name": "Submarine-Frontend",
  "author": "Yuma Satake",
  "version": "0.0.1",
  "type": "module",
  "main": "src/main.tsx",
  "engines": {
    "node": "18.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/submarine-dev/submarine.git"
  },
  "scripts": {
    "dev": "vite --mode dev --host",
    "build": "pnpm build:dev",
    "build:dev": "tsc && vite build --mode dev",
    "build:prod": "tsc && vite build --mode prod",
    "build:analyze": "tsc && vite build --mode dev",
    "lint": "biome lint --write ./ && tsc --noEmit",
    "format": "biome format --write ./",
    "check": "biome check --write ./ && tsc --noEmit"
  },
  "dependencies": {
    "@aspida/axios": "^1.14.0",
    "@emotion/css": "^11.13.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@tanstack/react-query": "^5.45.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "axios": "^1.7.2",
    "jotai": "^2.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-oauth-popup": "^1.0.5",
    "react-router-dom": "^6.23.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite": "^5.4.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/node": "^22.5.4",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "dotenv": "^16.4.5",
    "typescript": "^5.2.2"
  }
}
